<?xml version="1.0"?>
<rdf:RDF xmlns="https://github.com/Stefan-Driessen/ProMoTe#"
     xml:base="https://github.com/Stefan-Driessen/ProMoTe"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:pmt="https://github.com/Stefan-Driessen/ProMoTe#pmt:"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dcam="http://purl.org/dc/dcam/"
     xmlns:dcat="http://www.w3.org/ns/dcat#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="https://vocab.org/vann/#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="http://schema.org/"
     xmlns:dcmi-terms="https://www.dublincore.org/specifications/dublin-core/dcmi-terms/#">
    <owl:Ontology rdf:about="https://github.com/Stefan-Driessen/ProMoTe">
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/dcat"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/prov-o-20130430"/>
        <rdfs:isDefinedBy>https://github.com/Stefan-Driessen/ProMoTe</rdfs:isDefinedBy>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/terms/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description">
        <skos:definition xml:lang="en">A human-readable account of the resource.</skos:definition>
        <pmt:motivation xml:lang="en">Descriptions make the resource more Understandable. Additionally, descriptions can contribute to Discoverability if they are indexed in a data catalog.</pmt:motivation>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title">
        <skos:definition xml:lang="en">A name given to the item.</skos:definition>
        <pmt:motivation xml:lang="en">A title contributes to Discoverability, Addressability (if unique), and Understandability.</pmt:motivation>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    </owl:AnnotationProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#Estimated_Value -->

    <owl:AnnotationProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#Estimated_Value">
        <rdfs:comment xml:lang="en">An indication of the value the resource provides for the company.</rdfs:comment>
        <rdfs:label xml:lang="en">estimated value</rdfs:label>
        <pmt:motivation xml:lang="en">Explicitly including an estimated value is necessary for establishing that a resource or distribution is Valuable.</pmt:motivation>
        <vann:usageNote xml:lang="en">Ideally, the estimated value is backed by a quantifier, such as money or manhours saved. Otherwise, a qualitative description of the value provided can be provided.</vann:usageNote>
        <vann:usageNote xml:lang="en">In commercial settings estimated value can be used as the price of the resource.</vann:usageNote>
    </owl:AnnotationProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#knowledgeURI -->

    <owl:AnnotationProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#knowledgeURI">
        <rdfs:comment xml:lang="en">A link to documentation about the institutional knowledge.</rdfs:comment>
        <rdfs:label>knowledge URI</rdfs:label>
        <vann:usageNote xml:lang="en">Best practice is to store institutional knowledge in a business glossary or domain ontologies.</vann:usageNote>
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-institutional-knowledge"/>
    </owl:AnnotationProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#pmt:consumeInstructions -->

    <owl:AnnotationProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#pmt:consumeInstructions">
        <rdfs:comment xml:lang="en">Human-readable instructions on how to consume data through this output port.</rdfs:comment>
        <rdfs:label xml:lang="en">consume instructions</rdfs:label>
        <rdfs:seeAlso>https://www.w3.org/TR/vocab-dcat-2/#Property:data_service_endpoint_description</rdfs:seeAlso>
        <pmt:motivation xml:lang="en">Consume instructions help with making the data Natively Accessible, Reusable, and, potentially, Interoperable.</pmt:motivation>
        <vann:usageNote xml:lang="en">Consume instructions can serve as the informal couterpart to the pmt:dataContract, which captures the formal promises and expectations of this output port.</vann:usageNote>
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-output-port"/>
    </owl:AnnotationProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#pmt:consumerPromise -->

    <owl:AnnotationProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#pmt:consumerPromise">
        <rdfs:comment xml:lang="en">A promise required from the data consumer to the pmt:dataProvider that is captured in the data contract.</rdfs:comment>
        <rdfs:label xml:lang="en">consumer promise</rdfs:label>
        <pmt:motivation xml:lang="en">Consumer promises contribute to making data products Feedback-Driven and Security.</pmt:motivation>
        <vann:usageNote xml:lang="en">Consumer promises can also be captured in a pmt:SLA, pmt:SLO or in a pmt:policy.</vann:usageNote>
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-contract"/>
    </owl:AnnotationProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#pmt:motivation -->

    <owl:AnnotationProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#pmt:motivation"/>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#pmt:plannedEndDate -->

    <owl:AnnotationProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#pmt:plannedEndDate">
        <rdfs:comment xml:lang="en">The date by which the use case plans to stop consuming data products.</rdfs:comment>
        <rdfs:label xml:lang="en">planned end date</rdfs:label>
        <pmt:motivation xml:lang="en">Planned end dates of use cases are important for establishing Value and achieving Feedback-Driven development.</pmt:motivation>
        <vann:usageNote xml:lang="en">If there is no foreseeable end date, the planned end date can be indefinite.</vann:usageNote>
        <vann:usageNote xml:lang="en">Keeping track of a planned end date helps with data product maintenance. and versioning. There is no need to put effort into maintaining a data product if there are no active use cases.</vann:usageNote>
    </owl:AnnotationProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#pmt:providerPromise -->

    <owl:AnnotationProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#pmt:providerPromise">
        <rdfs:comment xml:lang="en">A promise made by the pmt:dataProvider to the data consumer that is captured in the data contract.</rdfs:comment>
        <rdfs:label xml:lang="en">provider promise</rdfs:label>
        <pmt:motivation xml:lang="en">Provider promises contribute to Truthfulness &amp; Trustworthiness, Native Accessibility and Security.</pmt:motivation>
        <vann:usageNote xml:lang="en">Provider promises can also be captured in a pmt:SLA, pmt:SLO or in a pmt:policy.</vann:usageNote>
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-contract"/>
    </owl:AnnotationProperty>
    


    <!-- https://vocab.org/vann/#usageNote -->

    <owl:AnnotationProperty rdf:about="https://vocab.org/vann/#usageNote">
        <rdfs:label>vann:usageNote</rdfs:label>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- dct:identifier -->

    <owl:ObjectProperty rdf:about="dct:identifier">
        <rdfs:label xml:lang="en">identifier</rdfs:label>
        <skos:definition xml:lang="en">An unambiguous reference to the resource within a given context.</skos:definition>
        <pmt:motivation xml:lang="en">A unique identifier is a necessity for Addressability.</pmt:motivation>
        <vann:usageNote xml:lang="en">Common values are a urn or url.</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- http://xmlns.com/foaf/0.1/primaryTopic -->

    <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/primaryTopic"/>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#appliesToPort -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#appliesToPort">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-contract"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-input-port"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-output-port"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The output port to which this policy applies.</rdfs:comment>
        <rdfs:label xml:lang="en">applies to port</rdfs:label>
        <pmt:motivation xml:lang="en">Linking data contracts to output ports makes data products Feedback-Driven, Truthful &amp; Trustworthy, Valuable and Secure.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#control-port-property-policy -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#control-port-property-policy">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-control-port"/>
        <rdfs:comment>A policy that can be managed through this control port.</rdfs:comment>
        <rdfs:label xml:lang="en">managed policy</rdfs:label>
        <vann:usageNote>A wide variety of policies may exist that manage different aspects of the data product such as: computational policies, data product standardized protocols, and automated tests and automated monitoring.</vann:usageNote>
        <vann:usageNote>Best practice is to use additional documentation to describe and manage the different types of policies that exist within a data mesh ecosystem.</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#data-contract-property-policy -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#data-contract-property-policy">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-contract"/>
        <rdfs:comment>A policy that is explained and enforced in the data contract.</rdfs:comment>
        <rdfs:label xml:lang="en">policy</rdfs:label>
        <pmt:motivation>Policies can contribute to one or more of Understandability, Truthfulness &amp; Trustworthiness and Security.</pmt:motivation>
        <vann:usageNote>A wide variety of policies may exist that manage different aspects of the data product such as: computational policies, data product standardized protocols, and automated tests and automated monitoring.</vann:usageNote>
        <vann:usageNote>Best practice is to use additional documentation to describe and manage the different types of policies that exist within a data mesh ecosystem.</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#data-product-property-output-port -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#data-product-property-output-port">
        <owl:inverseOf rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#property-exposes-data-product"/>
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-product"/>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-output-port"/>
        <rdfs:comment xml:lang="en">An output port that exposes a distribution of this data product.</rdfs:comment>
        <rdfs:label xml:lang="en">output port</rdfs:label>
        <pmt:motivation xml:lang="en">Output ports make different distributions of a data product&apos;s datasets Natively Accessible in a Reusable manner. Additionally, tracking lineage in general contributes to Discoverability, Addressability, Understandability, and Interoperability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#dataset-property-logical-schema -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#dataset-property-logical-schema">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-dataset"/>
        <rdfs:comment>A description of the data structure and internal relations at the dataset-level.</rdfs:comment>
        <rdfs:label xml:lang="en">logical schema</rdfs:label>
        <pmt:motivation>Logical schemas contribute to Understandability and Interoperability.</pmt:motivation>
        <vann:usageNote>Logical schemas describe the structure of data that holds true across different distributions of the dataset. Structural descriptions of different distributions are described in pmt:technicalSchema.</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#distribution-property-output-port -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#distribution-property-output-port">
        <owl:inverseOf rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#output-port-property-distribution"/>
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-output-port"/>
        <rdfs:comment xml:lang="en">An output port that exposes this distribution.</rdfs:comment>
        <rdfs:label xml:lang="en">corresponding output port</rdfs:label>
        <pmt:motivation xml:lang="en">Output ports make different distributions of a data product&apos;s datasets Natively Accessible and Reusable. Additionally, tracking lineage in general contributes to Discoverability, Addressability, Understandability, and Interoperability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#endpointURI -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#endpointURI">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-output-port"/>
        <rdfs:seeAlso xml:lang="en">dcat:endpointURL</rdfs:seeAlso>
        <skos:definition xml:lang="en">A unique URI for the endpoint of this output port.</skos:definition>
        <pmt:motivation xml:lang="en">Unique endpoints contribute to Addressability and Native Accessibility.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#enforcementLogic -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#enforcementLogic">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-product"/>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#EnforcementLogic"/>
        <rdfs:comment xml:lang="en">The logic or code used to (semi-)automatically enforce the policies that govern the data product.</rdfs:comment>
        <rdfs:label>enforcement logic</rdfs:label>
        <pmt:motivation xml:lang="en">Automating enforcement logic contributes to the Truthfulness &amp; Trustworthiness of the data product.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#input-port-property-corresponding-data-product -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#input-port-property-corresponding-data-product">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-control-port"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-input-port"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment>The data product of this input port.</rdfs:comment>
        <rdfs:label xml:lang="en">corresponding data product</rdfs:label>
        <pmt:motivation xml:lang="en">Keeping track of who consumes what data contributes to determining Value, as well as establishing data lineage. In addition to making data products Feedback-Driven, tracking lineage in general contributes to Discoverability, Addressability, Understandability, and Interoperability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#output-port-property-distribution -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#output-port-property-distribution">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-output-port"/>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
        <rdfs:comment xml:lang="en">The distributions that are exposed for consumption through this output port.</rdfs:comment>
        <rdfs:label xml:lang="en">exposes distribution</rdfs:label>
        <pmt:motivation xml:lang="en">Grouping together multiple distributions per output port helps with Native Accessibility and Reusability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-control-port -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-control-port">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-product"/>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-control-port"/>
        <rdfs:comment xml:lang="en">A control port through which the policies of this data product are managed.</rdfs:comment>
        <rdfs:label xml:lang="en">control port</rdfs:label>
        <pmt:motivation xml:lang="en">Control ports are primarily used to monitor the Truthfulness &amp; Trustworthiness of data products. They can also be used to enforce Interoperability and Security.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-data-contract -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-data-contract">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-input-port"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-output-port"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment xml:lang="en">A data contract associated with an output port. If the output port is the input port of another data contract, the data contract regulates how the data flows into that data product.</rdfs:comment>
        <rdfs:label xml:lang="en">data contract</rdfs:label>
        <pmt:motivation xml:lang="en">Data Contracts contribute to Understandability, Truthfulness &amp; Trustworthiness, Native Accessibility and Security.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-data-provider -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-data-provider">
        <rdfs:subPropertyOf rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#property-resource-provider"/>
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-product"/>
        <rdfs:comment>The actor responsible for providing the data product.</rdfs:comment>
        <rdfs:label xml:lang="en">data provider</rdfs:label>
        <pmt:motivation>Data providers are essential to achieve Addressability, as well as making the development of the resource Feedback-Driven.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-dataset -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-dataset">
        <owl:inverseOf rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#property-offered-in-data-product"/>
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-product"/>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-dataset"/>
        <rdfs:comment>A dataset that is offered through this data product.</rdfs:comment>
        <rdfs:label xml:lang="en">data asset</rdfs:label>
        <pmt:motivation xml:lang="en">Grouping one or more data assets together in a data product helps with Reusability, Discoverability, Addressability, Truthfulness and Trustworthiness, Understandability and Interoperability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-defining-domain -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-defining-domain">
        <owl:inverseOf rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#property-domain-knowledge"/>
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-institutional-knowledge"/>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-domain"/>
        <rdfs:comment xml:lang="en">The domain that defines and maintains the institutional knowledge.</rdfs:comment>
        <rdfs:label xml:lang="en">defining domain</rdfs:label>
        <vann:usageNote xml:lang="en">It is possible that a piece of institutional knowledge is maintained on an organisation-wide level, rather than on a domain-level, but there should still be a domain responsible for maintining organisation-wide knowledge.</vann:usageNote>
        <vann:usageNote xml:lang="en">This is an inverse property of pmt:institutionalKnowledge</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-domain -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-domain">
        <owl:inverseOf rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#property-resource"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-domain"/>
        <rdfs:label xml:lang="en">domain</rdfs:label>
        <skos:definition xml:lang="en">The domain from which the resource stems.</skos:definition>
        <pmt:motivation xml:lang="en">Domains make resources more Addressable and Discoverable.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-domain-knowledge -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-domain-knowledge">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-domain"/>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-institutional-knowledge"/>
        <rdfs:comment xml:lang="en">Existing terminology within the organisation or domain that relates to a pmt:Resource.</rdfs:comment>
        <rdfs:label xml:lang="en">domain knowledge</rdfs:label>
        <pmt:motivation xml:lang="en">Institutional knowledge contributes to Discoverability, Understandability and Interoperability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-exposes-data-product -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-exposes-data-product">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-output-port"/>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-product"/>
        <rdfs:comment>The data product that this output port exposes for consumption.</rdfs:comment>
        <rdfs:label xml:lang="en">exposes data product</rdfs:label>
        <pmt:motivation xml:lang="en">Having multiple output ports per data product helps with Native Accessibility and Reusability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-input-port -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-input-port">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-product"/>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-input-port"/>
        <rdfs:comment xml:lang="en">An input port through which data is ingested into this data product.</rdfs:comment>
        <rdfs:label xml:lang="en">input port</rdfs:label>
        <pmt:motivation xml:lang="en">In addition to making data products Feedback-Driven, tracking lineage in general contributes to Discoverability, Addressability, Understandability, and Interoperability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-newer-version -->

