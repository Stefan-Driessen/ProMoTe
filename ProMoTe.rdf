                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A newer version of the data product, which will eventually replace this version.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property is used for versioning, keeping an older version available helps consumers of the resources by giving them time to migrate to the new resource.</rdfs:comment>
        <rdfs:label xml:lang="en">has new version</rdfs:label>
        <rdfs:seeAlso xml:lang="en">http://purl.org/dc/terms/hasVersion</rdfs:seeAlso>
        <pmt:motivation xml:lang="en">Versioning in general contributes to Understandability, Truthfulness &amp; Trustworthiness, Interoperability, Native Accessibility, and Reusability.</pmt:motivation>
        <vann:usageNote xml:lang="en">Recommended usage is to describe until when this resource is available in the dct:description property.</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-offered-in-data-product -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-offered-in-data-product">
