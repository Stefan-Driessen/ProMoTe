        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-product"/>
        <rdfs:comment>A data product that makes this dataset available for consumption.</rdfs:comment>
        <rdfs:label xml:lang="en">offered in data product</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-older-version -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-older-version">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">An older version of the data product, which this version is replacing.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property is used for versioning, keeping an older version available helps consumers of the resources by giving them time to migrate to the new resource.</rdfs:comment>
        <rdfs:label xml:lang="en">has old version</rdfs:label>
        <rdfs:seeAlso xml:lang="en">http://purl.org/dc/terms/isVersionOf</rdfs:seeAlso>
        <pmt:motivation xml:lang="en">Versioning in general contributes to Understandability, Truthfulness &amp; Trustworthiness, Interoperability, Native Accessibility, and Reusability.</pmt:motivation>
        <vann:usageNote xml:lang="en">Recommended usage is to describe when the old resource is no longer available in the dct:description property.</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-physical-schema -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-physical-schema">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
        <rdfs:comment>A description of the data structure and internal relations at the distribution-level.</rdfs:comment>
        <rdfs:label xml:lang="en">physical schema</rdfs:label>
        <pmt:motivation>Physical schemas help with Understandability and Interoperability.</pmt:motivation>
        <vann:usageNote xml:lang="en">Physical schemas describe the structure of data of a specific distribution of a pmt:DataAseset. Structural descriptions that hold true across all distributions of a dataset are described in pmt:logicalSchema.</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-resource -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-resource">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-domain"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>A resource offered in the domain.</rdfs:comment>
        <rdfs:label xml:lang="en">has resource</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-resource-provider -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-resource-provider">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:label xml:lang="en">resource provider</rdfs:label>
        <skos:definition xml:lang="en">The agent responsible for providing the resource.</skos:definition>
        <pmt:motivation xml:lang="en">Resource providers are necessary to achieve Addressability, as well as making the development of the resource Feedback-Driven.</pmt:motivation>
        <vann:usageNote xml:lang="en">Resource Providers are either data product providers or use case providers.</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-resource-type -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-resource-type">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
        <rdfs:comment xml:lang="en">The type of the resource, either a data product, data asset or a use case</rdfs:comment>
        <rdfs:label xml:lang="en">type</rdfs:label>
        <rdfs:seeAlso>https://www.dublincore.org/specifications/dublin-core/dcmi-terms/#http://purl.org/dc/terms/type</rdfs:seeAlso>
        <pmt:motivation xml:lang="en">Describing the type of resource contributes to Discoverability and Understandability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-service-level-agreement -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-service-level-agreement">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-contract"/>
        <rdfs:comment>A service level agreement related to the delivery of data through the output port of this data contract.</rdfs:comment>
        <rdfs:label xml:lang="en">SLA</rdfs:label>
        <pmt:motivation>Service level agreements contribute to Understandability, Truthfulness &amp; Trustworthiness, Native Accessibility and Security.</pmt:motivation>
        <vann:usageNote>Best practice is to use additional documentation to describe and manage SLAs in the data mesh ecosystem.</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#property-service-level-objective -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#property-service-level-objective">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-contract"/>
        <rdfs:comment>A service level objective related to the delivery of data through the output port of this data contract.</rdfs:comment>
        <rdfs:label xml:lang="en">SLO</rdfs:label>
        <pmt:motivation>Service level objectives contribute to Understandability, Truthfulness &amp; Trustworthiness, Native Accessibility and Security.</pmt:motivation>
        <vann:usageNote>Best practice is to use additional documentation to describe and manage SLOs in the data mesh ecosystem.</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#resource-property-consumed-by -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#resource-property-consumed-by">
        <owl:inverseOf rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#resource-property-consumes"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-output-port"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-input-port"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-use-case"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>An input port of a data product or a use case that consumes this data product.</rdfs:comment>
        <rdfs:label xml:lang="en">is consumed by</rdfs:label>
        <pmt:motivation>In addition to making data products Feedback-Driven, tracking lineage in general contributes to Discoverability, Addressability, Understandability, and Interoperability.</pmt:motivation>
        <vann:usageNote>Linking consuming data products helps establish data lineage, whereas linking use cases to data products improves the discoverability, understandability of the data product and helps establish a pmt:estimatedValue</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#resource-property-consumes -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#resource-property-consumes">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-input-port"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-output-port"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A resource or output port of a data product which is consumed to create or maintain this resource or input port.</rdfs:comment>
        <rdfs:label xml:lang="en">consumes</rdfs:label>
        <pmt:motivation xml:lang="en">In addition to making data products Feedback-Driven, tracking lineage in general contributes to Discoverability, Addressability, Understandability, and Interoperability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#resource-property-institutional-knowledge -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#resource-property-institutional-knowledge">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-resource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-institutional-knowledge"/>
        <rdfs:comment xml:lang="en">Existing terminology within the organisation or domain that relates to a pmt:Resource.</rdfs:comment>
        <rdfs:label xml:lang="en">institutional knowledge</rdfs:label>
        <pmt:motivation xml:lang="en">Institutional knowledge contributes to Discoverability, Understandability and Interoperability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#resource-property-source-system -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#resource-property-source-system">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-dataset"/>
                    <rdf:Description rdf:about="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment xml:lang="en">An IT system that generates or stores data in this resource, such as a speficic data warehouse, data lake, or database.</rdfs:comment>
        <rdfs:label xml:lang="en">source system</rdfs:label>
        <pmt:motivation xml:lang="en">In addition to making data products Feedback-Driven, tracking lineage in general contributes to Discoverability, Addressability, Understandability, and Interoperability.</pmt:motivation>
        <vann:usageNote xml:lang="en">This property can be used to describe either where a specific data asset or distribution lives or give information on the input port of a data product.</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#pmt:accessManagement -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#pmt:accessManagement">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-output-port"/>
        <rdfs:label xml:lang="en">access management</rdfs:label>
        <skos:definition xml:lang="en">The management and enforcement of access rights on an output port.</skos:definition>
        <pmt:motivation xml:lang="en">Access Mangement contributes to Security, Native Accessibility and Feedback-Driven.</pmt:motivation>
        <vann:usageNote xml:lang="en">Access Management is usually provided by the organisation as part of a larger IAM framework</vann:usageNote>
        <vann:usageNote xml:lang="en">Access management and enforcement should also be described in the pmt: DataContract associated with the pmt:OutputPort</vann:usageNote>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#pmt:catalogRecord -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#pmt:catalogRecord">
        <rdfs:range rdf:resource="http://www.w3.org/ns/dcat#CatalogRecord"/>
        <rdfs:label xml:lang="en">catalog record</rdfs:label>
        <skos:definition xml:lang="en">The record describing this resource in a data catalog</skos:definition>
        <pmt:motivation xml:lang="en">Catalog records contribute to Discoverability, Addressability, Understandability, Interoperability and Value.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#pmt:distribution -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#pmt:distribution">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/dcat#distribution"/>
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-dataset"/>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-distribution"/>
        <rdfs:label xml:lang="en">distribution</rdfs:label>
        <skos:definition xml:lang="en">A physical manifestion of (a subset of) the data asset.</skos:definition>
        <pmt:motivation xml:lang="en">Distributions make data assets Natively Accessible.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#pmt:domainOwner -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#pmt:domainOwner">
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Person"/>
        <rdfs:label>domain owner</rdfs:label>
        <skos:definition xml:lang="en">The person who carries the high-level responsibility for domain, the business processes in it and the data they generate.</skos:definition>
        <pmt:motivation xml:lang="en">Domain owners help assign responsibility for the different data products, which contributes to Addressability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#pmt:domainParticipant -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#pmt:domainParticipant">
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:label>domain participant</rdfs:label>
        <skos:definition xml:lang="en">A person who is active within the domain.</skos:definition>
        <pmt:motivation xml:lang="en">Domain participants can have various relations to the data product and its data. Registering them contributes to Addressability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://github.com/Stefan-Driessen/ProMoTe#pmt:transformationLogic -->

    <owl:ObjectProperty rdf:about="https://github.com/Stefan-Driessen/ProMoTe#pmt:transformationLogic">
        <rdfs:domain rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#class-data-product"/>
        <rdfs:range rdf:resource="https://github.com/Stefan-Driessen/ProMoTe#TransformationLogic"/>
        <rdfs:comment xml:lang="en">The logic or code used to transform data coming in from the input ports of the data product to the distributions that are exposerd through the output ports.</rdfs:comment>
        <rdfs:label xml:lang="en">transformation logic</rdfs:label>
        <pmt:motivation>Transformation logic is necessary to create distributions that are Natively Accessible and guarantee a certain level of Truthfulness and Trustworthiness and/or Interoperability.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://www.dublincore.org/specifications/dublin-core/dcmi-terms/#issued -->

    <owl:ObjectProperty rdf:about="https://www.dublincore.org/specifications/dublin-core/dcmi-terms/#issued">
        <rdfs:comment xml:lang="en">Recommended practice is to describe the date, date/time, or period of time as recommended for the property Date, of which this is a subproperty.</rdfs:comment>
        <rdfs:label xml:lang="en">Date Issued</rdfs:label>
        <skos:definition xml:lang="en">Date of formal issuance of the resource.</skos:definition>
        <pmt:motivation xml:lang="en">Adding the date issued contributes to the Understandability of the resource.</pmt:motivation>
    </owl:ObjectProperty>
    


    <!-- https://www.dublincore.org/specifications/dublin-core/dcmi-terms/#modified -->

    <owl:ObjectProperty rdf:about="https://www.dublincore.org/specifications/dublin-core/dcmi-terms/#modified">
        <rdfs:comment xml:lang="en">Recommended practice is to describe the date, date/time, or period of time as recommended for the property Date, of which this is a subproperty.</rdfs:comment>
        <rdfs:label xml:lang="en">Date Modified</rdfs:label>
        <skos:definition xml:lang="en">Date on which the resource was changed.</skos:definition>
        <pmt:motivation xml:lang="en">The date modified contributes to the Understandability, Interoperability, and Trustworthiness &amp; Truthfulness.</pmt:motivation>
    </owl:ObjectProperty>
    


